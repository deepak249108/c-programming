}
void deepak()
{
    int a,b,sum;
    printf("eneter your number a: \n");
    scanf("%d",&a);
    printf("eneter your number b: \n");
    scanf("%d",&b);
    sum=a*b;
    printf("the sum is %d \n",a*b);
}
